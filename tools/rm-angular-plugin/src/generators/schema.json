{
  "$schema": "https://json-schema.org/schema",
  "$id": "Component",
  "title": "",
  "type": "object",
  "properties": {
    "path": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "Component name"
    },
    "project": {
      "enum": ["ui", "landing-page", "runmates-app", "runmates-web"],
      "type": "string",
      "x-prompt": "Choose a project"
    },
    "required": ["name"]
  }
}
